PORT=4000
MONGODB_URI=mongodb://localhost:27017/services_dev
JWT_SECRET=dev_jwt_secret_change_me_please
JWT_ACCESS_EXPIRES_IN=1d
JWT_REFRESH_EXPIRES_IN=30d
JWT_REFRESH_SECRET=dev_refresh_secret_change_me
# Optional CORS override (comma-separated origins or *)
CORS_ORIGIN=http://localhost:3000
# Alternatively, allowlist multiple origins (takes precedence over CORS_ORIGIN)
# CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Google OAuth Client ID for verifying Google ID tokens (required for Google login)
# Obtain from https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=

# Base URL of the web app used for links in emails
WEB_BASE_URL=http://localhost:3000

# Email provider (SES via SMTP)
EMAIL_PROVIDER=ses
SES_SMTP_HOST=email-smtp.us-east-1.amazonaws.com
SES_SMTP_PORT=587
SES_SMTP_USER=
SES_SMTP_PASS=
EMAIL_FROM=
EMAIL_FROM_NAME=

# Aster DEX integration
ASTERDEX_ENABLED=false
ASTERDEX_BASE_URL=https://fapi.asterdex.com
ASTERDEX_POLL_INTERVAL_MS=60000
# MongoDB ObjectId of the user that should own synced trades
ASTERDEX_USER_ID=
# Wallet + signer data for authenticated requests
ASTERDEX_USER_ADDRESS=
ASTERDEX_SIGNER_ADDRESS=
ASTERDEX_SIGNER_PRIVATE_KEY=
# Optional override for recvWindow (max 60000)
ASTERDEX_RECV_WINDOW_MS=5000
